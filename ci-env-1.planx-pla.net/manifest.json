  "sower": [
    {
      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/metadata-manifest-ingestion:2021.01",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "get-dbgap-metadata",
      "action": "get-dbgap-metadata",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/get-dbgap-metadata:2021.01",
        "pull_policy": "Always",
        "env": [],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/manifest-indexing:2021.01",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:2021.01",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
