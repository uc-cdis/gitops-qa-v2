{
  "notes": [
    "This is the ci-env-1 jenkins environment manifest",
    "That's all I have to say."
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "arborist": "quay.io/cdis/arborist:2021.08",
    "audit-service": "quay.io/cdis/audit-service:2021.08",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "awshelper": "quay.io/cdis/awshelper:2021.08",
    "fence": "quay.io/cdis/fence:2021.08",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "indexd": "quay.io/cdis/indexd:2021.08",
    "mariner": "quay.io/cdis/mariner-server:2021.08",
    "ws-storage": "quay.io/cdis/ws-storage:2021.08",
    "peregrine": "quay.io/cdis/peregrine:2021.08",
    "pidgin": "quay.io/cdis/pidgin:2021.08",
    "portal": "quay.io/cdis/data-portal:2021.08",
    "revproxy": "quay.io/cdis/nginx:2021.08",
    "sheepdog": "quay.io/cdis/sheepdog:2021.08",
    "spark": "quay.io/cdis/gen3-spark:2021.08",
    "tube": "quay.io/cdis/tube:2021.08",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2021.08",
    "manifestservice": "quay.io/cdis/manifestservice:2021.08",
    "wts": "quay.io/cdis/workspace-token-service:2021.08",
    "guppy": "quay.io/cdis/guppy:2021.08",
    "sower": "quay.io/cdis/sower:2021.08",
    "hatchery": "quay.io/cdis/hatchery:2021.08",
    "metadata": "quay.io/cdis/metadata-service:2021.08",
    "requestor": "quay.io/cdis/requestor:2021.08"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "sower": [
    {
      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/metadata-manifest-ingestion:2021.08",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "get-dbgap-metadata",
      "action": "get-dbgap-metadata",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/get-dbgap-metadata:2021.08",
        "pull_policy": "Always",
        "env": [],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/manifest-indexing:2021.08",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-ci-env-1-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:2021.08",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:2021.08",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "subject"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "peregrine": {
    "sidecar": "True"
  },
  "global": {
    "environment": "qaplanetv2",
    "hostname": "ci-env-1.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/anvil/master/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv2-gen3",
    "logs_bucket": "logs-qaplanetv2-gen3",
    "sync_from_dbgap": "True",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "dispatcher_job_num": "5",
    "netpolicy": "on"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:2021.08"
    }
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "jenkins_subject_alias",
        "type": "subject"
      },
      {
        "index": "jenkins_file_alias",
        "type": "file"
      }
    ],
    "config_index": "jenkins_configs_alias",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 2,
      "max": 3,
      "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  }
}
